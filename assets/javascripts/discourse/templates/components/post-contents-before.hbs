{{#if site.mobileView}}
    {{#if model.postBeforeFixedCountAdsense}}
        {{#if siteSettings.adsense_mobile_post_contents_before_code}}
            {{google-adsense placement="post-contents-before" postNumber=model.post_number}}
        {{/if}}
    {{/if}}
    {{#if model.postBefore2FixedCountAdsense}}
        {{#if siteSettings.adsense_mobile_post_contents_before_2_code}}
            {{google-adsense placement="post-contents-before-2" postNumber=model.post_number}}
        {{/if}}
    {{/if}}
{{else}}
    {{#if model.postBeforeFixedCountAdsense}}
        {{#if siteSettings.adsense_post_contents_before_code}}
            {{google-adsense placement="post-contents-before" postNumber=model.post_number}}
        {{/if}}
    {{/if}}
    {{#if model.postBefore2FixedCountAdsense}}
        {{#if siteSettings.adsense_post_contents_before_2_code}}
            {{google-adsense placement="post-contents-before-2" postNumber=model.post_number}}
        {{/if}}
    {{/if}}
{{/if}}
