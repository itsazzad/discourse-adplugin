{{#if site.mobileView}}
  {{#if model.postFixedCountAdsense}}
    {{#if siteSettings.adsense_mobile_post_before_code}}
      {{google-adsense placement="post-before" postNumber=model.post_number}}
    {{/if}}
  {{/if}}
{{else}}
  {{#if model.postFixedCountAdsense}}
    {{#if siteSettings.adsense_post_before_code}}
      {{google-adsense placement="post-before" postNumber=model.post_number}}
    {{/if}}
  {{/if}}
{{/if}}
